{
  "name": "probiller-ng/transaction-service",
  "description": "ProBiller NG Transaction Service",
  "keywords": [
    "Transaction Service",
    "Transaction",
    "ProBillerNG"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repository.mgcorp.co/artifactory/api/composer/cose-php-libraries"
    },
    {
      "type": "composer",
      "url": "https://mtl1-satis.mgcorp.co"
    },
    {
      "type": "composer",
      "url": "https://repository.mgcorp.co/artifactory/api/composer/probiller-php-libraries"
    }
  ],
  "type": "project",
  "require": {
    "php": ">=7.3.0",
    "ext-apcu": "*",
    "ext-grpc": "*",
    "ext-json": "*",
    "ext-redis": "*",
    "doctrine/common": "^2.10",
    "google/cloud-firestore": "^1.19",
    "google/protobuf": "^3.15",
    "grpc/grpc": "^1.36",
    "guzzlehttp/guzzle": "~6.0",
    "inacho/php-credit-card-validator": "^1.0",
    "jms/serializer": "^2.1",
    "laravel/lumen-framework": "^6.0",
    "mindgeek/aad": "^1.3",
    "moneyphp/money": "^3.3",
    "myclabs/php-enum": "^1.7",
    "probiller-ng/base-lumen-middleware": "^2.1",
    "probiller-ng/bi-logger": "^2.0",
    "probiller-ng/circuit-breaker": "^1.0",
    "probiller-ng/legacy-service-client": "^1.0",
    "probiller-ng/logger": "^2.0",
    "probiller-ng/netbilling-service": "^1.5",
    "probiller-ng/pumapay-service": "^1.2",
    "probiller-ng/qysso-service": "^1.0",
    "probiller-ng/rocketgate-service": "1.13.2",
    "probiller/service-config": "6.8.1",
    "ramsey/uuid": "^3.8",
    "vlucas/phpdotenv": "^3.3"
  },
  "require-dev": {
    "ext-apc": "*",
    "appzcoder/lumen-routes-list": "^1.0",
    "fzaninotto/faker": "~1.4",
    "mikey179/vfsstream": "^1.6",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~8.0",
    "probiller-ng/secret-manager": "^2.1",
    "slevomat/coding-standard": "^4.7",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "lumen/app/",
      "ProBillerNG\\": "src/",
      "src\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "archive": {
    "exclude": [
      "/tests/",
      "/public/docs/"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
